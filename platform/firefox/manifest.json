{
  "author": "",
  "homepage_url": "http://rednoise.org/adlipo/",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "browser_style": false,
    "default_icon": {
      "16": "AdLiPoImage/icon-16.png",
      "32": "AdLiPoImage/icon-38.png"
    },
    "default_title": "AdLiPo",
    "default_popup": "popup-fenix.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "adlipo@adlipo",
      "strict_min_version": "57.0"
    }
  },
  "commands": {
    "_execute_browser_action": {
    },
    "launch-element-zapper": {
      "description": "__MSG_popupTipZapper__"
    },
    "launch-element-picker": {
      "description": "__MSG_popupTipPicker__"
    },
    "launch-logger": {
      "description": "__MSG_popupTipLog__"
    },
    "open-dashboard": {
      "description": "__MSG_popupTipDashboard__"
    },
    "relax-blocking-mode": {
      "description": "__MSG_relaxBlockingMode__"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "js": [
        "/js/rita-micro.js",
        "/js/vapi.js",
        "/js/vapi-client.js",
        "/js/contentscript.js",
        "/js/injectTemplate.js"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://easylist.to/*",
        "https://*.fanboy.co.nz/*",
        "https://filterlists.com/*",
        "https://forums.lanik.us/*",
        "https://github.com/*",
        "https://*.github.io/*"
      ],
      "js": [
        "/js/scriptlets/subscriber.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extShortDesc__",
  "icons": {
    "16": "AdLiPoImage/icon-16.png",
    "32": "AdLiPoImage/icon-38.png",
    "48": "AdLiPoImage/icon-48.png",
    "128": "AdLiPoImage/icon-128.png"
  },
  "manifest_version": 2,
  "name": "AdLiPo",
  "options_ui": {
    "page": "dashboard.html",
    "open_in_tab": true
  },
  "permissions": [
    "dns",
    "menus",
    "privacy",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "AdLiPo",
  "sidebar_action": {
    "default_title": "__MSG_statsPageName__",
    "default_panel": "logger-ui.html",
    "default_icon": "AdLiPoImage/AdLiPo.svg",
    "open_at_install": false
  },
  "version": "dev",
  "web_accessible_resources": [
    "/web_accessible_resources/*"
  ]
}
