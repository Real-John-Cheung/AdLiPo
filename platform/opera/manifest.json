{
  "author": " ",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_icon": {
      "16": "AdLiPoImage/icon-16.png",
      "32": "AdLiPoImage/icon-38.png"
    },
    "default_popup": "popup-fenix.html",
    "default_title": "AdLiPo"
  },
  "commands": {
    "launch-element-picker": {
      "description": "__MSG_popupTipPicker__"
    },
    "launch-element-zapper": {
      "description": "__MSG_popupTipZapper__"
    },
    "launch-logger": {
      "description": "__MSG_popupTipLog__"
    },
    "open-dashboard": {
      "description": "__MSG_popupTipDashboard__"
    },
    "relax-blocking-mode": {
      "description": "__MSG_relaxBlockingMode__"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": [
        "/js/rita-micro.js",
        "/js/vapi.js",
        "/js/vapi-client.js",
        "/js/contentscript.js",
        "/js/injectTemplate.js"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://easylist.to/*",
        "https://*.fanboy.co.nz/*",
        "https://filterlists.com/*",
        "https://forums.lanik.us/*",
        "https://github.com/*",
        "https://*.github.io/*"
      ],
      "js": [
        "/js/scriptlets/subscriber.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extShortDesc__",
  "icons": {
    "16": "AdLiPoImage/icon-16.png",
    "32": "AdLiPoImage/icon-38.png",
    "48": "AdLiPoImage/icon-48.png",
    "128": "AdLiPoImage/icon-128.png"
  },
  "incognito": "split",
  "manifest_version": 2,
  "minimum_opera_version": "42.0",
  "name": "AdLiPo",
  "options_page": "dashboard.html",
  "permissions": [
    "contextMenus",
    "privacy",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>"
  ],
  "short_name": "AdLiPo",
  "sidebar_action": {
    "default_icon": {
      "16": "AdLiPoImage/icon-16.png",
      "32": "AdLiPoImage/icon-32.png"
    },
    "default_panel": "logger-ui.html",
    "default_title": "__MSG_statsPageName__"
  },
  "version": "1.14.23.17",
  "web_accessible_resources": [
    "/web_accessible_resources/*"
  ]
}
